  "bugs": {
    "url": "https://github.com/anturson/start-api-pub/issues"
  },
  "homepage": "https://github.com/anturson/start-api-pub#readme",
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "numeral": "^2.0.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc16",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  }
