{
  "name": "start-api-pub",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel src -d dist",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "start": "npm-run-all --parallel lint:watch test:watch start-watch",
    "start-watch": "nodemon src/index.js --exec babel-node",
    "serve": "node dist/index.js",
    "start-prod": "npm-run-all build serve",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress \"src/**/**.test.js\" --require @babel/register",
    "test:watch": "npm run test -- --watch",
    "heroku-create": "heroku create",
    "heroku-remote": "heroku git:remote -a sleepy-dusk-61958",
    "heroku-deploy": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anturson/start-api-pub.git"
  },
  "keywords": [],
